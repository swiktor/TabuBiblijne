<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.rzeszow.swiktor.tabuteokratyczne">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@mipmap/ic_launcher_moje"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_moje_round"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="pl.rzeszow.swiktor.tabuteokratyczne.MainActivity"
            android:label="Gra" />
        <activity
            android:name="pl.rzeszow.swiktor.tabuteokratyczne.BiblioteczkaActivity"
            android:label="Biblioteczka internetowa" />
        <activity
            android:name="pl.rzeszow.swiktor.tabuteokratyczne.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pl.rzeszow.swiktor.tabuteokratyczne.RankingActivity"
            android:label="Ranking" />
    </application>

</manifest>